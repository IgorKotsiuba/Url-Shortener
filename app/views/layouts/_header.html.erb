<% if user_signed_in? %>
  <li><a><%= current_user.email %></a></li>
  <li><%= link_to 'Update Profile', edit_user_registration_path, class: "active" %></li>
  <li><%= link_to 'Logout', destroy_user_session_path, :method => :delete, class: "active" %></li>
<% else %>
    <li><%= link_to 'Sign In', new_user_session_path, id: 'Sign In', class: "active" %></li>
    <li><%= link_to 'Sign Up', new_user_registration_path, class: "active" %></li>
<% end %>
